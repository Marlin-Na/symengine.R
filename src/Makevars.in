
CXX_STD = CXX11

PKG_LIBS = @@@PKG_LIBS@@@

PKG_CPPFLAGS = @@@PKG_CPPFLAGS@@@

.PHONY: all @@@DO_UPSTREAM@@@

all: $(SHLIB)

$(OBJECTS): @@@DO_UPSTREAM@@@

## 'make install' to put a copy of header files and static library
## to "inst/cpplib${R_ARCH}".
do_upstream:
	@(cd ../cmakebuild${R_ARCH} && $(MAKE) && $(MAKE) install)
