
# Links against the downloaded binaries
PKG_CPPFLAGS= -I../win-symengine/Library/include -I../win-mpc/Library/include
PKG_LIBS= -L../win-symengine/Library/lib -L../win-mpc/Library/lib -lsymengine -lmpc -lmpfr -lgmp

all: clean winlibs

clean:
	rm -f $(OBJECTS) $(SHLIB) 

winlibs:
	"${R_HOME}/bin${R_ARCH_BIN}/Rscript.exe" "../tools/winlibs.R"

