
CXX_STD = CXX11

PKG_LIBS = -L../cmakebuild${R_ARCH}/symengine -lsymengine -lmpfr -lgmp

PKG_CPPFLAGS = -I../src/upstream -I../cmakebuild${R_ARCH}

.PHONY: all do_upstream

all: $(SHLIB)

